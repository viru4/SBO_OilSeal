# Production Environment Variables for Backend
# Copy this to .env in your backend deployment

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Admin Token (Generate a secure random string)
ADMIN_TOKEN=your_secure_admin_token_here

# Production Settings
NODE_ENV=production
PORT=8080

# CORS Origins (Add your deployed frontend URL)
CORS_ORIGINS=https://your-frontend-domain.netlify.app,https://your-custom-domain.com